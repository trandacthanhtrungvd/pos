{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@context/*": ["src/context/*"],
      "@routes/*": ["src/routes/*"],
      "@assets/*": ["src/assets/*"],
      "@images/*": ["src/assets/images/*"],
      "@icons/*": ["src/assets/icons/*"],
      "@/*": ["src/*"]
    }
  },
  "exclude": ["node_modules", "public"]
}
